{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": {
          "enabled": false
        }
      },
      "_meta": {
        "version": "5.3.0"
      },
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "name": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "version": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "fields": {
          "properties": {}
        },
        "meta": {
          "properties": {
            "cloud": {
              "properties": {
                "availability_zone": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "instance_id": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "machine_type": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "project_id": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "provider": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "region": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                }
              }
            }
          }
        },
        "rtt": {
          "type": "double"
        },
        "tags": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "target_addr": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "target_description": {
          "index": "analyzed",
          "norms": {
            "enabled": false
          },
          "type": "string"
        },
        "target_name": {
          "index": "analyzed",
          "norms": {
            "enabled": false
          },
          "type": "string"
        },
        "target_tags": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "pingbeat-*"
}